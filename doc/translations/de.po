"\n"
"@export_group(\"Car Properties\", \"car_\")\n"
"@export var car_label = \"Speedy\"\n"
"@export var car_number = 3\n"
"\n"
"@export_group(\"\", \"\")\n"
"@export var ungrouped_number = 3\n"
